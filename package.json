{
  "name": "@agentwatch/parsing",
  "version": "0.1.0",
  "description": "Parse AI agent transcripts (Claude Code, Codex CLI, Gemini CLI) into a unified format",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "transcript",
    "parsing",
    "claude",
    "claude-code",
    "codex",
    "codex-cli",
    "gemini",
    "gemini-cli",
    "agent",
    "ai",
    "llm"
  ],
  "author": "nmvg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmvg/agentwatch-parsing.git"
  },
  "homepage": "https://github.com/nmvg/agentwatch-parsing#readme",
  "bugs": {
    "url": "https://github.com/nmvg/agentwatch-parsing/issues"
  }
}
